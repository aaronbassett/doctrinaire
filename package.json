{
  "name": "doctrinaire",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "help": "bun ./src/utils/run-help.ts",
    "dev": "vite",
    "prepare": "bunx husky",
    "lint": "bun run biome lint",
    "lint:fix": "bun run biome lint --write",
    "format": "bun run biome format",
    "format:fix": "bun run biome format --write",
    "check": "bun run biome check",
    "check:changed": "bun run biome check --changed --no-errors-on-unmatched",
    "check:staged": "bun run biome check --staged --no-errors-on-unmatched",
    "check:fix": "bun run biome check --fix",
    "check:changed:fix": "bun run biome check --changed --no-errors-on-unmatched --fix",
    "check:staged:fix": "bun run biome check --staged --no-errors-on-unmatched --fix"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "motion": "^12.0.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwind-merge": "^2.6.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@tailwindcss/vite": "^4.0.0",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.3.4",
    "chalk": "^5.4.1",
    "commitizen": "^4.3.1",
    "commitlint-config-gitmoji": "^2.3.1",
    "cz-emoji-conventional": "^1.1.0",
    "daisyui": "^5.0.0-beta.2",
    "husky": "^9.1.7",
    "tailwindcss": "^4.0.0",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.18.2",
    "vite": "^6.0.5",
    "vite-plugin-toml": "^0.7.0"
  }
}
